<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/beta/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
	<Frame name="TargetedSpellsPartyPreviewTemplate" mixin="TargetedSpellsPartyPreviewMixin" virtual="true">
		<Size y="1" />
		<Frames>
			<Button name="RaidFrameSettingsPreviewFrame" parentKey="RaidFrame" inherits="CompactUnitFrameTemplate">
				<KeyValues>
					<KeyValue key="ignoreCUFNameRequirement" value="true" type="boolean" />
					<KeyValue key="unit" value="preview" type="string" />
				</KeyValues>
				<!-- <Anchors>
					<Anchor point="TOPRIGHT" x="-60" y="-20"/>
				</Anchors> -->
			</Button>
		</Frames>
		<Layers>
			<Layer level="OVERLAY">
				<FontString text="PREVIEW" inherits="GameFontHighlightSmall">
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativeKey="$parent.RaidFrame" relativePoint="TOPLEFT" x="0" y="6" />
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad method="OnLoad" />
		</Scripts>
	</Frame>
</Ui>